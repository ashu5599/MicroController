#include <stdint.h>
#include <stdio.h>
#include "stm32f4xx.h"
#include "system_stm32f4xx.h"

#include "switch.h"
#include "timer.h"


int main(void)
{
    int duty ;
    SystemInit();
    TimerInit();
    SwitchInit(duty);
    while(1) {
        for(duty=0; duty<=100;duty+=10)
        {
            TIM8->CCR1 = duty;
            DelayMs(50);
        }
        for(duty=100; duty>=1; duty-=10) {
            TIM8->CCR1 = duty;
            DelayMs(50);
        }

    }
	return 0;
}
